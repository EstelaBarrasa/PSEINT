Funcion fact <- numFact(num)
	definir fact , i como entero
	fact=1
	Para i<-1 Hasta num Con Paso 1 Hacer
		fact=fact*i
	Fin Para
Fin Funcion


Funcion suma <- numSuma ( num1,num2 )
	definir suma , i Como Entero
	suma=0
	Si num1<num2 Entonces
		Para i<-num1+1 Hasta num2-1 Con Paso 1 Hacer
			suma=suma+i
		Fin Para
	Sino
		Para i<-num2+1 Hasta num2-1 Con Paso 1 Hacer
			suma=suma+i
		Fin Para
	Fin Si	
Fin Funcion


Funcion may <- numMay(num1,num2)
	definir may Como Entero
	may = 0
	
	si num1>num2 Entonces
		may = num1
	Sino
		may = num2
	FinSi
Fin Funcion


Funcion men <- numMen(num1,num2)
	definir men como entero
	men = 0
	
	Si num1<num2 Entonces
		men = num1
	Sino
		men = num2
	Fin Si
Fin Funcion




